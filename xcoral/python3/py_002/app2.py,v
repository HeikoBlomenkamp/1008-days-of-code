phrase = "Giraffe Academy"

print("[01] " + phrase.lower())
print("[02] " + phrase.upper())
print("[03] phrase.isupper()")
print(phrase.isupper())

print("[04] phrase.upper().isupper()")
print(phrase.upper().isupper())
